{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit: run app.py",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": ["run", "app.py", "--server.port", "8501", "--server.headless", "true"],
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}